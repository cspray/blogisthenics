{
  "name": "cspray/blogisthenics",
  "description": "Put your blog generation on a diet with Blogisthenics!",
  "require": {
    "php": "^8.1",
    "laminas/laminas-escaper": "^2.10.0",
    "league/commonmark": "^2.3",
    "minicli/minicli": "^3.2",
    "minicli/command-help": "^0.1.0",
    "voku/stringy": "^6.5",
    "adbario/php-dot-notation": "^3.1",
    "cspray/annotated-container": "^1.0",
    "rdlowrey/auryn": "^1.4",
    "league/flysystem": "^3.1",
    "league/flysystem-memory": "^3.0"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Blogisthenics\\": "src",
      "Cspray\\Blogisthenics\\Cli\\": "cli_src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Blogisthenics\\Test\\": "test",
      "Cspray\\BlogisthenicsFixture\\": "fixture_src"
    }
  },
  "bin": [
    "bin/blogisthenics"
  ]
}
