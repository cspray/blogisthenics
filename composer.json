{
  "name": "cspray/blogisthenics",
  "description": "A static site generator built to be highly-extensible",
  "require": {
    "amphp/amp": "^2",
    "amphp/file": "^0.3",
    "zendframework/zend-escaper": "^2.6",
    "danielstjules/stringy": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Blogisthenics\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\Blogisthenics\\Test\\": "test"
    }
  }
}
